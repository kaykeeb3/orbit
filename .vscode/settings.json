  "editor.formatOnSave": true,
  "editor.defaultFormatter": "biomejs.biome",
  "biome.validate.enable": true,
  "biome.organizeImports": true,
  "biome.autoFixOnSave": true,
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
